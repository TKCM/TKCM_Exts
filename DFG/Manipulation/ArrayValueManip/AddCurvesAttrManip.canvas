        ],
      "definition" : {
        "objectType" : "Func",
        "title" : "func",
        "ports" : [
          {
            "objectType" : "ExecPort",
            "name" : "exec",
            "nodePortType" : "IO",
            "execPortType" : "IO",
            "typeSpec" : "Execute"
            },
          {
            "objectType" : "ExecPort",
            "name" : "this",
            "nodePortType" : "Out",
            "execPortType" : "In",
            "typeSpec" : "Curves"
            },
          {
            "objectType" : "ExecPort",
            "name" : "result",
            "nodePortType" : "In",
            "execPortType" : "Out",
            "typeSpec" : "Xfo[]"
            }
          ],
        "extDeps" : {},
        "code" : "dfgEntry {
  result.resize(this.allCurvesPointRange());
  
  Size poiCount = 0;
  for(Integer i=0; i<this.curveCount(); i++){
    for(Integer j=0; j<this.getCurve(i).pointCount(); j++){
      Float32 lengthRatio = 1.0 / (this.getCurve(i).pointCount()-1) * i;
      Float32 nParam = this.getNormalizedParameterFromLengthRatio(i, lengthRatio);
      if(j == 0){  nParam = 0.0;  }
      Vec3 pos = this.getPointPosition(poiCount);
      
      Quat ori;
      Vec3 tangent = this.getTangentAtNormalizedParam(i, nParam);
      Vec3 normal;
      if(this.getCurveType(i) == 1 || this.getDegree(i) == 1){
        normal = Vec3(0, 1, 0);
      }else{
        if(this.attributes.has(\"normals\")){
          Ref<Vec3Attribute> normalsAttr = this.attributes.getAttribute(\"normals\");
          UInt32 startID = this.getFirstPointIndex(i);
          normal = normalsAttr.values[j+startID];
          if(normal == Vec3(0)){
            normal = this.getNormalAtNormalizedParam(i, nParam);
          }
        }else{
          normal = this.getNormalAtNormalizedParam(i, nParam);
        }
      }
      ori.setFromDirectionAndUpvector(tangent, normal);
      result[poiCount] = Xfo(pos, ori);     
      poiCount += 1;
    }
  }
  
}
"
        }
      },
